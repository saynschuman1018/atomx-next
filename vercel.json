{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "sameorigin"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "default-src https: 'unsafe-eval' 'unsafe-inline'; img-src https: data:; object-src 'none'"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "frame-ancestors https://app.storyblok.com/"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1"
        }
      ]
    }
  ]
}
